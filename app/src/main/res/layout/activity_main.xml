<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EFEFEF"
    tools:context=".MainActivity">

    <TextView

        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="30dp"
        android:gravity="center"
        android:text="Parolni kiriting"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="HardcodedText" />

    <ImageView
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@drawable/shape_circle"
        android:contentDescription="TODO"

        android:src="@drawable/mine_icon"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="ContentDescription,HardcodedText" />

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="50dp"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">


        <TextView
            android:id="@+id/textPw1"
            android:layout_width="30dp"
            android:layout_height="50dp"
            android:layout_marginEnd="0dp"
            android:background="@drawable/shape_radius_border_1"
            android:gravity="center"
            android:text="⚪️"
            android:textSize="20sp"
            tools:ignore="HardcodedText,TextSizeCheck,TextContrastCheck" />

        <TextView
            android:id="@+id/textPw2"
            android:layout_width="30dp"
            android:layout_height="50dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/shape_radius_border_1"
            android:gravity="center"
            android:text="⚪️"
            android:textSize="20sp"
            tools:ignore="HardcodedText,TextSizeCheck,TextContrastCheck" />

        <TextView
            android:id="@+id/textPw3"
            android:layout_width="30dp"
            android:layout_height="50dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/shape_radius_border_1"
            android:gravity="center"
            android:text="⚪️"
            android:textSize="20sp"
            tools:ignore="HardcodedText,RtlHardcoded,TextSizeCheck,TextContrastCheck" />

        <TextView
            android:id="@+id/textPw4"
            android:layout_width="30dp"
            android:layout_height="50dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/shape_radius_border_1"
            android:gravity="center"
            android:text="⚪️"
            android:textSize="20sp"
            tools:ignore="HardcodedText,TextSizeCheck,TextContrastCheck" />


    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="340dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/appBtn1"
                android:layout_width="70dp"
                android:layout_height="80dp"
                android:layout_marginRight="30dp"
                android:background="@drawable/shape_radius_border_1"
                android:text="1"
                android:textSize="20sp"
                tools:ignore="HardcodedText,RtlHardcoded" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/appBtn2"
                android:layout_width="70dp"
                android:layout_height="80dp"
                android:layout_marginStart="30dp"
                android:layout_marginEnd="30dp"
                android:background="@drawable/shape_radius_border_1"
                android:text="2"
                android:textSize="20sp"
                tools:ignore="HardcodedText" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/appBtn3"
                android:layout_width="70dp"
                android:layout_height="80dp"
                android:layout_marginLeft="30dp"
                android:background="@drawable/shape_radius_border_1"
                android:text="3"
                android:textSize="20sp"
                tools:ignore="HardcodedText,RtlHardcoded" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:gravity="center"
            android:orientation="horizontal">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/appBtn4"
                android:layout_width="70dp"
                android:layout_height="80dp"
                android:layout_marginRight="30dp"
                android:background="@drawable/shape_radius_border_1"
                android:text="4"
                android:textSize="20sp"
                tools:ignore="HardcodedText,RtlHardcoded" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/appBtn5"
                android:layout_width="70dp"
                android:layout_height="80dp"
                android:layout_marginStart="30dp"
                android:layout_marginEnd="30dp"
                android:background="@drawable/shape_radius_border_1"
                android:text="5"
                android:textSize="20sp"
                tools:ignore="HardcodedText" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/appBtn6"
                android:layout_width="70dp"
                android:layout_height="80dp"
                android:layout_marginLeft="30dp"
                android:background="@drawable/shape_radius_border_1"
                android:text="6"
                android:textSize="20sp"
                tools:ignore="HardcodedText,RtlHardcoded" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:gravity="center"
            android:orientation="horizontal">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/appBtn7"
                android:layout_width="70dp"
                android:layout_height="80dp"
                android:layout_marginRight="30dp"
                android:background="@drawable/shape_radius_border_1"
                android:text="7"
                android:textSize="20sp"
                tools:ignore="HardcodedText,RtlHardcoded" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/appBtn8"
                android:layout_width="70dp"
                android:layout_height="80dp"
                android:layout_marginStart="30dp"
                android:layout_marginEnd="30dp"
                android:background="@drawable/shape_radius_border_1"
                android:text="8"
                android:textSize="20sp"
                tools:ignore="HardcodedText" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/appBtn9"
                android:layout_width="70dp"
                android:layout_height="80dp"
                android:layout_marginLeft="30dp"
                android:background="@drawable/shape_radius_border_1"
                android:text="9"
                android:textSize="20sp"
                tools:ignore="HardcodedText,RtlHardcoded" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:gravity="center"
            android:orientation="horizontal">


            <ImageView
                android:id="@+id/imageBtnBack"
                android:layout_width="70dp"
                android:layout_height="80dp"
                android:layout_marginStart="30dp"
                android:layout_marginEnd="30dp"
                android:background="@drawable/shape_radius_border_1"
                android:padding="10dp"
                android:src="@drawable/back_2"
                tools:ignore="ContentDescription" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/appBtn0"
                android:layout_width="70dp"
                android:layout_height="80dp"
                android:layout_marginStart="30dp"
                android:layout_marginEnd="30dp"
                android:background="@drawable/shape_radius_border_1"
                android:text="0"
                android:textSize="20sp"
                tools:ignore="HardcodedText" />

            <ImageView
                android:id="@+id/barmoqIziBtn"
                android:layout_width="70dp"
                android:layout_height="80dp"
                android:layout_marginStart="30dp"
                android:layout_marginEnd="30dp"
                android:background="@drawable/shape_radius_border_1"
                android:padding="10dp"
                android:src="@drawable/barmoq_izi"
                tools:ignore="ContentDescription" />

        </LinearLayout>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>